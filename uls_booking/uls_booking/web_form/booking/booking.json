 "client_script": "frappe.web_form.after_load = async () => {\n\n    \n    \n    let contacts = [];\n    await frappe.call({\n        method: \"uls_booking.uls_booking.api.api.get_email_table\",\n        args: {\n            email_id : frappe.session.user,\n        },\n        callback: function (r) {\n            if (r.message) {\n                contacts = r.message;  \n            }\n        },\n    });\n\n\n    let customers = [];\n    for (let contact of contacts)\n    {\n        await frappe.call({\n            method: \"uls_booking.uls_booking.api.api.get_customer\",\n            args: {\n                contact : contact,\n            },\n            callback: function (r) {\n                if (r.message) {\n                        r.message.forEach(customer => {\n                            customers.push(customer);\n                        });\n                }\n            },\n        });\n    }\n    customers = [...new Set(customers)];\n    \n    var options = [];\n    for (var i = 0; i < customers.length; i++) {\n    \n        options.push({\n            'label': customers[i],\n            'value': customers[i]\n        });\n    }\n\n    frappe.web_form.set_value(\"customer\", null);\n    var cust_field = frappe.web_form.get_field(\"customer\");\n    cust_field._data = options;\n    cust_field.refresh();\n        \n};\n\n\n\n",
 "condition_json": "[]",
